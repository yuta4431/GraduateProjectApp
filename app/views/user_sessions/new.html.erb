<h1>UserSessions#new</h1>
<%= form_with url: login_path do |f| %>
  <div class="mb-3">
    <%= f.label :ユーザー名 %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class="mb-3">
    <%= f.label :パスワード %>
    <%= f.password_field :password, class: 'form-control' %>
  </div>
  <%= f.submit :ログイン, class: 'btn btn-primary' %>
<% end %>

